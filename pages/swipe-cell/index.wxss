.page {
  background: #f8f8f8;
  padding-top: 100rpx;
}

.swipe-btns {
  display: flex;
  align-items: center;
  width: 100%;
  height: 100%;
}

.swipe-btn {
  flex: 1;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #2d8cf0;
  color: #fff;
}

.swipe-btn:last-child {
  background: #e00;
}

.title {
  font-size: 26rpx;
  color: #abc;
  padding: 20rpx;
}